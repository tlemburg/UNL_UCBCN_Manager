/*	-------------------------------------------------------------
	IE 7
	- - - - - - - - - - - - - - - - - - - - - - - - - - - - - - -	*/
*+html #maincontent form fieldset.eventFieldsetAction label, *+html #maincontent form fieldset.eventFieldsetAction legend{
	position:absolute;top:-9999px;display:inline;
}

*+html #maincontent form fieldset legend {
	margin: -13px 0 0 -7px;
}

*+html body#normal div#maincontent ul.eventsbystatus {
	margin: -27px 0 10px 0;;
	padding: 6px 6px 0 6px;
}

*+html #maincontent form fieldset.d__header___class {
	margin-top: 4px;
}
*+html body#subscribe #maincontent form fieldset.d__header___class{margin:40px 0 0 0;}
*+html #maincontent form fieldset {
	margin: 20px 0;
	padding: 0 0 10px 0;
}
*+html #maincontent form fieldset li {
	float:none;
}